package rules;

import com.athub.rules.entity.Person;

rule "person_1"
when
$person:Person(age < 30);
then
$person.setScore(80);
System.out.println("触发规则，30岁以下得80分");
end

rule "person_2"
when
$person:Person(30 < age && age< 40)
then
$person.setScore(70);
System.out.println("触发规则，30岁-40岁得70分");
end
